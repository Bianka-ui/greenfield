{
  "http-server": {
    "port": 8080,
    "socket-timeout": 6000,
    "static-dirs": [
      {
        "http-path": "/apps/icons",
        "fs-path": "./app-entries/"
      },
      {
        "http-path": "/stats",
        "fs-path": "./stats/"
      }
    ]
  },
  "png-encoder": {
    "max-target-buffer-size": 65536
  },
  "desktop-shell": {
    "apps-controller": {
      "app-entries-urls": [
        "file:./app-entries/"
      ]
    }
  },
  "stats": {
    "output": "./stats/stats.js",
    "enabled": false
  }
}
